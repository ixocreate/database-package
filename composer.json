{
    "name": "ixocreate/database-package",
    "description": "IXOCREATE Database Package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE",
            "email": "opensource@ixocreate.com",
            "homepage": "https://www.ixocreate.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "doctrine/dbal": "^2.6",
        "doctrine/migrations": "^3.0",
        "doctrine/orm": "^2.9.0",
        "ixocreate/application": "^0.3.0",
        "ixocreate/entity-package": "^0.3.0"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^2.0",
        "ixocreate/test": "^0.2"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Database\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ixocreate\\Misc\\Database\\": "tests/misc/",
            "Ixocreate\\Test\\Database\\": "tests/src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
